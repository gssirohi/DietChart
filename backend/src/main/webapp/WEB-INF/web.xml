<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">
    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>SystemServiceServlet</servlet-name>
        <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
        <init-param>
            <param-name>services</param-name>
            <param-value>com.techticz.dietchart.backend.endpoints.MyEndpoint,
                com.techticz.dietchart.backend.endpoints.FoodEntityEndpoint,
                com.techticz.dietchart.backend.endpoints.BlobEndPoint,
                com.techticz.dietchart.backend.endpoints.MealEntityEndpoint,
                com.techticz.dietchart.backend.endpoints.MealPlanEntityItemEndpoint,
                com.techticz.dietchart.backend.endpoints.AppUserEndpoint,
                </param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>SystemServiceServlet</servlet-name>
        <url-pattern>/_ah/spi/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BlobUploadServlet</servlet-name>
        <servlet-class>com.techticz.dietchart.backend.servlets.BlobUploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BlobUploadServlet</servlet-name>
        <url-pattern>/blob/upload/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BlobDownloadServlet</servlet-name>
        <servlet-class>com.techticz.dietchart.backend.servlets.BlobDownloadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BlobDownloadServlet</servlet-name>
        <url-pattern>/blob/serve</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>